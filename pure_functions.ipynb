   "source": [
    "def counter( folder, filename):\n",
    "    import os \n",
    "    count = 0\n",
    "    with os.scandir(folder) as entries:\n",
    "        for entry in entries:           \n",
    "            if filename in entry.name:\n",
    "                count += 1\n",
    "    return count\n"
   ]
